delete: olcObjectClasses
-
add: olcObjectClasses
olcObjectClasses: ( 1.2.40.0.13.1.15.111.4.1 NAME 'dcmuiConfig'
  DESC 'Archive UI Configuration'
  SUP top STRUCTURAL
  MUST (
    dcmuiConfigName ) )
olcObjectClasses: ( 1.2.40.0.13.1.15.111.4.2 NAME 'dcmuiPermission'
  DESC 'Permission for UI Action'
  SUP top STRUCTURAL
  MUST (
    dcmuiPermissionName $
    dcmuiAction )
  MAY (
    dcmuiActionParam $
    dcmAcceptedUserRole ) )
olcObjectClasses: ( 1.2.40.0.13.1.15.111.4.3 NAME 'dcmuiDiffConfig'
  DESC 'UI Configuration for comparing studies in two archives'
  SUP top STRUCTURAL
  MUST (
    dcmuiDiffConfigName )
  MAY (
    dcmuiDiffCallingAET $
    dcmuiDiffPrimaryCFindSCP $
    dcmuiDiffPrimaryCMoveSCP $
    dcmuiDiffPrimaryCStoreSCP $
    dcmuiDiffSecondaryCFindSCP $
    dcmuiDiffSecondaryCMoveSCP $
    dcmuiDiffSecondaryCStoreSCP ) )
olcObjectClasses: ( 1.2.40.0.13.1.15.111.4.4 NAME 'dcmuiDiffCriteria'
  DESC 'Criteria for comparing studies in two archives'
  SUP top STRUCTURAL
  MUST (
    dcmuiDiffCriteriaTitle )
  MAY (
    dicomDescription $
    dcmuiDiffCriteriaNumber $
    dcmuiDiffIncludeMissing $
    dcmAttributeSetID $
    dcmuiDiffAction $
    dcmuiDiffGroupButton ) )
olcObjectClasses: ( 1.2.40.0.13.1.15.111.4.5 NAME 'dcmuiDashboardConfig'
  DESC 'UI Dashboard Config'
  SUP top STRUCTURAL
  MUST (
    dcmuiDashboardConfigName )
  MAY (
    dcmuiQueueName $
    dcmuiExportName $
    dcmuiCountAET $
    dicomuiDeviceName $
    dicomuiIgnoreParams $
    dcmuiCompareSideName) )
olcObjectClasses: ( 1.2.40.0.13.1.15.111.4.6 NAME 'dcmuiElasticsearchConfig'
    DESC 'UI Configuration for Elasticsearch'
    SUP top STRUCTURAL
    MUST (
      dcmuiElasticsearchConfigName )
    MAY (
      dcmuiElasticsearchURL ) )
olcObjectClasses: ( 1.2.40.0.13.1.15.111.4.7 NAME 'dcmuiElasticsearchURLObjects'
    DESC 'Elasticsearch URL'
    SUP top STRUCTURAL
    MUST (
      dcmuiElasticsearchURLName $
      dcmuiElasticsearchURL )
    MAY (
      dcmuiElasticsearchIsDefault $
      dcmuiElasticsearchInstalled) )
olcObjectClasses: ( 1.2.40.0.13.1.15.111.4.8 NAME 'dcmuiDeviceURLObject'
    DESC 'Device URL'
    SUP top STRUCTURAL
    MUST (
      dcmuiDeviceURLName $
      dcmuiDeviceURL)
    MAY (
      dcmuiDeviceURLDescription $
      dcmuiDeviceURLInstalled) )
olcObjectClasses: ( 1.2.40.0.13.1.15.111.4.9 NAME 'dcmuiDeviceClusterObject'
    DESC 'Device Cluster'
    SUP top STRUCTURAL
    MUST (
      dcmuiDeviceClusterName $
      dcmuiDeviceClusterDevices)
    MAY (
      dcmuiDeviceClusterDescription $
      dcmuiDeviceClusterLoadBalancer $
      dcmuiDeviceClusterKeycloakServer $
      dcmuiDeviceClusterInstalled) )
olcObjectClasses: ( 1.2.40.0.13.1.15.111.4.10 NAME 'dcmuiCompareSideObjects'
  DESC 'Dashboard Side Compare'
  SUP top STRUCTURAL
  MUST (
    dcmuiCompareSideName $
    dcmuiCompareSideElasticsearch $
    dcmuiCompareSideCluster )
  MAY (
    dcmuiCompareSideInstalled $
    dcmuiCompareSideQueueName $
    dcmuiCompareSideDescription) )
