  "paths": {
    "/aets/{aet}/rs/patients": {
      "get": {
        "tags": [
          "QIDO-RS"
        ],
        "summary": "Search for Patients",
        "operationId": "SearchForPatients",
        "produces": [
          "multipart/related;type=application/dicom+xml",
          "application/dicom+json"
        ],
        "parameters": [
          {
            "name": "aet",
            "in": "path",
            "description": "Archive AE Title",
            "required": true,
            "type": "string"
          },
          {
            "name": "{attributeID}",
            "in": "query",
            "description": "{attributeID}={value}; {attributeID} = {dicomTag} | {dicomKeyword} | {dicomTag}.{attributeID} | {dicomKeyword}.{attributeID}",
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          },
          {
            "name": "includefield",
            "in": "query",
            "description": "includefield={attributeID} | all",
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          },
          {
            "name": "orderby",
            "in": "query",
            "description": "orderby=[-]{attributeID}",
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          },
          {
            "name": "withoutstudies",
            "in": "query",
            "description": "return Patient w/o Studies",
            "type": "boolean",
            "default": true
          },
          {
            "name": "fuzzymatching",
            "in": "query",
            "description": "fuzzy semantic matching of person names",
            "type": "boolean",
            "default": false
          },
          {
            "name": "offset",
            "in": "query",
            "description": "offset={skippedResults}",
            "type": "integer",
            "default": 0
          },
          {
            "name": "limit",
            "in": "query",
            "description": "limit={maximumResults}",
            "type": "integer"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "file"
            }
          },
          "204": {
            "description": "no matching patient found"
          },
          "default": {
            "description": "unexpected error"
          }
        }
      }
    },
    "/aets/{aet}/rs/studies": {
