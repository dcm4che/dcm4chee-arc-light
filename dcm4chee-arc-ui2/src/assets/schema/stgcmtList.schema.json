      },
      "seriesUID": {
        "title": "Series Instance UID",
        "description": "Unique Identifier of the Series",
        "type": "string"
      },
      "objectUID": {
        "title": "SOP Instance UID",
        "description": "Unique Identifier of the Object",
        "type": "string"
      },
      "exporterID": {
        "title": "Exporter ID",
        "description": "Exporter ID to filter by",
        "type": "string"
      },
      "requested": {
        "title": "Requested",
