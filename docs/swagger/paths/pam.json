  "diff.pdq.patients": {
    "get": {
      "tags": [
        "PDQ-RS"
      ],
      "summary": "Compare Patient Demographics",
      "operationId": "comparePatientDemographics",
      "parameters": [
        {
          "$ref": "../parameters.json#/archiveAETPathParam"
        },
        {
          "$ref": "../parameters.json#/pdqServiceIDPathParam"
        },
